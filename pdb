import threading
class SquareThread(threading.Thread):
    def __init__(self, n):
        threading.Thread.__init__(self)
        self.n = n

    def run(self):
        squares = [num**2 for num in range(1, self.n + 1)]
        print("Square Numbers:", squares)
if __name__ == "__main__":
    n = 10000
 square_thread = SquareThread(n)
 square_thread.start()
 square_thread.join()
